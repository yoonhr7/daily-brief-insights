{
  "name": "daily-brief-insights",
  "version": "0.1.0",
  "description": "Batch analysis engine that explains economic events and IT changes",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test:notion": "npm run build && node test-notion.js",
    "test:news": "npm run build && node test-with-news.js"
  },
  "keywords": [
    "analysis",
    "news",
    "insight",
    "economy",
    "it"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@google/generative-ai": "^0.19.0",
    "@notionhq/client": "^2.2.14",
    "dotenv": "^16.6.1",
    "node-cron": "^3.0.3",
    "openai": "^6.16.0",
    "rss-parser": "^3.13.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
